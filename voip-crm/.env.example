# This file contains environment variables for the VoIP CRM backend.
# Copy this file to `.env` and replace the placeholder values with your
# actual credentials. Never commit your real secrets to version control.

# Twilio credentials
TWILIO_ACCOUNT_SID=ACXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_API_KEY=SKXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
TWILIO_API_SECRET=your_twilio_api_secret

# The SID of your TwiML application (create in the Twilio console)
TWILIO_TWIML_APP_SID=APXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Supabase configuration
SUPABASE_URL=https://your-supabase-instance.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# Application configuration
PORT=3000
